{
  "name": "suggestions",
  "version": "0.0.1",
  "description": "suggestions",
  "scripts": {
    "build": "npx tsc",
    "start": "npx tsc && node ./build/index.js",
    "dev": "nodemon --legacy-watch",
    "lint": "npx eslint . --ext .ts",
    "lint-and-fix": "npx eslint . --ext .ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "./node_modules/.bin/eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "commander": "^9.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-rate-limit": "^6.4.0",
    "husky": "^4.3.8",
    "mongoose": "^6.2.8",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "location",
    "suggestions"
  ],
  "author": "Vivek Yadav",
  "license": "ISC"
}
